{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "cedrikaagaard/memsave-frontend",
      "hostname": "frontend",
      "essential": false,
      "memory": 128
    },

    {
      "name": "api",
      "image": "cedrikaagaard/memsave-api",
      "hostname": "api",
      "essential": false,
      "memory": 1024
    },

    {
      "name": "router",
      "image": "cedrikaagaard/memsave-router",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "frontend", "api"
      ]
    }
  ]
}
